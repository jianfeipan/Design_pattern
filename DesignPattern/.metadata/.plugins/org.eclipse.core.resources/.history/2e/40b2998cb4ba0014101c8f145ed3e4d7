package prototype;

public abstract class Animal implements Prototype{
	private String name;
	public Animal(String name){
		this.name=name;
	}
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
	
	public abstract void cry();
	
	public Animal makeacopy(){
		return Animal()
	}
}
